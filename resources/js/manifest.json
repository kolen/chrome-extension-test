{
  "name": "chrome-extension-test",
  "version": "0.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
 "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["chrome_extension_test.js", "content.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["chrome_extension_test.js", "background.js", "content2.js"],
    "persistent": false
  },
  "manifest_version": 2
}
